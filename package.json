{
  "name": "editmode",
  "version": "1.0.9",
  "description": "Modern document editor",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --config electron-builder.yml",
    "build:mac": "electron-builder --mac --config electron-builder.yml",
    "build:win": "electron-builder --win --config electron-builder.yml",
    "build:linux": "electron-builder --linux --config electron-builder.yml",
    "dist": "electron-builder --publish=never --config electron-builder.yml",
    "release": "electron-builder --publish=always --config electron-builder.yml"
  },
  "author": "VMescape",
  "license": "ISC",
  "dependencies": {
    "docx": "^9.4.1",
    "docxtemplater": "^3.42.4",
    "electron-updater": "^6.6.2",
    "html-docx-js": "^0.3.1",
    "html-pdf": "^3.0.1",
    "mammoth": "^1.6.0",
    "pizzip": "^3.1.4",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1"
  }
}
